{
  "name": "react-typescript",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/basarat/cypress-course.git"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules package-lock.json && npm install",
    "start:server": "ts-node --project tsconfig.server.json src/server/server.ts",
    "start:app": "react-scripts start",
    "start:cypress": "cypress open",
    "start": "concurrently -k \"npm run start:server\" \"npm run start:app\" \"npm run start:cypress\"",
    "build:check": "tsc -p .",
    "build:server": "tsc -p ./tsconfig.server.json",
    "build:app": "react-scripts build",
    "build:cypress:run": "wait-on http://localhost:3000 && cypress run",
    "build:cypress": "concurrently -k \"serve build -l 3000\" \"npm run start:server\" \"npm run build:cypress:run\"",
    "build": "npm run build:check && npm run build:server && npm run build:app && npm run build:cypress"
  },
  "dependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/lowdb": "1.0.9",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/uuid": "8.0.0",
    "axios": "0.19.2",
    "concurrently": "5.2.0",
    "cors": "2.8.5",
    "cypress": "4.7.0",
    "cypress-react-unit-test": "4.4.2",
    "express": "4.17.1",
    "formstate": "1.3.0",
    "lowdb": "1.0.0",
    "mobx": "5.15.4",
    "mobx-react": "6.2.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "serve": "11.3.1",
    "takeme": "0.11.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.3",
    "typestyle": "2.1.0",
    "uuid": "8.1.0",
    "wait-on": "5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
